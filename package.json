{
  "name": "2024.10.115-uc8-checkin-academia",
  "version": "1.0.0",
  "description": "Desenvolver um sistema completo de controle de check-in e check-out de alunos em uma academia, utilizando **Express.js**, **Sequelize**, **PostgreSQL**, autenticação **JWT** e boas práticas de código em **JavaScript**.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js --watch",
    "db:migrate": "npx sequelize db:migrate",
    "db:migrate:undo": "npx sequelize db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize db:migrate:undo:all",
    "db:seed": "npx sequelize db:seed:all",
    "db:seed:undo": "npx sequelize db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "db:fresh": "npm run db:migrate:undo:all && npm run db:migrate",
    "db:status": "npx sequelize db:migrate:status",
    "db:setup": "node scripts/setup-database.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SergioCunha361/2024.10.115-UC8-checkin-academia.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SergioCunha361/2024.10.115-UC8-checkin-academia/issues"
  },
  "homepage": "https://github.com/SergioCunha361/2024.10.115-UC8-checkin-academia#readme",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.3"
  }
}
